  <animation>
  <type>rotate</type>
  <object-name>flaparm</object-name>
  <object-name>handle</object-name>
   <property>/controls/flight/flaps</property>
     <interpolation>
      <entry><ind>  0.000</ind><dep> 0.0</dep></entry>
      <entry><ind>  0.125</ind><dep> 8.0</dep></entry>
      <entry><ind>  0.250</ind><dep> 16.0</dep></entry>
      <entry><ind>  0.375</ind><dep> 24.0</dep></entry>
      <entry><ind>  0.500</ind><dep> 32.0</dep></entry>
      <entry><ind>  0.625</ind><dep> 40.0</dep></entry>
      <entry><ind>  0.750</ind><dep> 48.0</dep></entry>
      <entry><ind>  0.875</ind><dep> 56.0</dep></entry>
      <entry><ind>  1.000</ind><dep> 64.0</dep></entry>
     </interpolation>   
  <center>
   <x-m>0.68</x-m>
   <y-m>-0.09</y-m>
   <z-m>0.65</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
  <type>rotate</type>   
  <object-name>sbhandle</object-name> 
  <object-name>sbarm</object-name>   
   <property>/b737/controls/flight/spoilers-lever-pos</property> 
  <interpolation>
      <entry><ind>  0.0</ind><dep>0.0</dep></entry>
      <entry><ind>  1.0</ind><dep>4.0</dep></entry>
      <entry><ind>  5.0</ind><dep>40</dep></entry>
      <entry><ind>  6.0</ind><dep>50.0</dep></entry>       
     </interpolation>  
  <center>
   <x-m>0.68</x-m>
   <y-m>-0.011</y-m>
   <z-m>0.65</z-m>
  </center>
  <axis>
   <x>-0.0</x>
   <y>1</y>
   <z>0.0</z>
  </axis>
 </animation>





